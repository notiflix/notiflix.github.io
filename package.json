{
  "name": "notiflix-documentation",
  "homepage": "https://notiflix.github.io",
  "version": "1.0.0-beta.01",
  "private": true,
  "scripts": {
    "startup": "eslint src && tsc --resolveJsonModule --outDir .dev --target ES5 src/helpers/sitemap-generator/SitemapGenerator.ts && node .dev/src/helpers/sitemap-generator/SitemapGenerator.js",
    "lint": "eslint next.config.js && next lint",
    "dev": "yarn startup && next lint && next dev",
    "build": "yarn startup && next build",
    "start": "yarn startup && next start",
    "deploy": "yarn build && next export -o .dist"
  },
  "dependencies": {
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@ts-stack/markdown": "^1.4.0",
    "@types/js-yaml": "^4.0.3",
    "@types/react": "17.0.19",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "frontmatter-markdown-loader": "^3.6.3",
    "js-yaml": "^4.1.0",
    "react-icons": "^4.2.0",
    "sass": "^1.38.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "stylelint-webpack-plugin": "^3.0.1",
    "typescript": "4.3.5"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 4 versions",
      "not ie < 11",
      "not dead",
      "not op_mini all",
      "iOS >=7"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
